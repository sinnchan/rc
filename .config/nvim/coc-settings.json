{
  "suggest.noselect": false,
  "inlayHint.enable": false,
  "Lua.diagnostics.globals": [
    "vim"
  ],
  "rust-analyzer.rustfmt.extraArgs": [
    "--config",
    "tab_spaces=2"
  ],
  "flutter.workspaceFolder.ignore": [
    "~/.pub-cache/",
    "~/.config/",
    "~/.dotfiles/",
    "~/fvm"
  ],
  "flutter.sdk.searchPaths": [
    "~/fvm/default/bin/flutter"
  ]
}
