{
  "riverpod class": {
    "description": "snip riverpod class",
    "prefix": [
      "riverpod"
    ],
    "body": [
      "@riverpod",
      "class $1 extends _\\$$1 {",
      "\t@override",
      "\t$2 build() {",
      "\t\t$0",
      "\t}",
      "}"
    ]
  },
  "freezed class": {
    "description": "snip freezed class",
    "prefix": [
      "freezed"
    ],
    "body": [
      "@freezed",
      "class $1 with _\\$$1 {",
      "\tconst factory $1({",
      "\t\t$0",
      "\t}) = _$1;",
      "",
      "\tfactory $1.fromJson(Map<String, Object?> json) =>",
      "\t\t_\\$$1FromJson(json);",
      "}"
    ]
  },
  "freezed part": {
    "description": "snip freezed part",
    "prefix": [
      "part",
      "pf"
    ],
    "body": [
      "part '$TM_FILENAME_BASE.freezed.dart';"
    ]
  },
  "generate part": {
    "description": "snip generate part",
    "prefix": [
      "part",
      "pg"
    ],
    "body": [
      "part '$TM_FILENAME_BASE.g.dart';"
    ]
  }
}
