{
  "riverpod method": {
    "description": "snip riverpod method",
    "prefix": [
      "snip_riverpod_method"
    ],
    "body": [
      "@riverpod",
      "$2 $1($1Ref ref) {",
      "\tthrow UnimplementedError();",
      "}"
    ]
  },
  "riverpod class": {
    "description": "snip riverpod class",
    "prefix": [
      "snip_riverpod_class"
    ],
    "body": [
      "@riverpod",
      "class $1 extends _\\$$1 {",
      "\t@override",
      "\t$2 build() {",
      "\t\t$0",
      "\t}",
      "}"
    ]
  },
  "freezed class": {
    "description": "snip freezed class",
    "prefix": [
      "snip_freezed"
    ],
    "body": [
      "import 'package:freezed_annotation/freezed_annotation.dart';",
      "",
      "part '$TM_FILENAME_BASE.freezed.dart';",
      "part '$TM_FILENAME_BASE.g.dart';",
      "",
      "@freezed",
      "class $1 with _\\$$1 {",
      "\tconst factory $1({",
      "\t\t$0",
      "\t}) = _$1;",
      "",
      "\tfactory $1.fromJson(Map<String, Object?> json) =>",
      "\t\t_\\$$1FromJson(json);",
      "}"
    ]
  },
  "freezed class only": {
    "description": "snip freezed class only",
    "prefix": [
      "snip_freezed_only"
    ],
    "body": [
      "@freezed",
      "class $1 with _\\$$1 {",
      "\tconst factory $1({",
      "\t\t$0",
      "\t}) = _$1;",
      "",
      "\tfactory $1.fromJson(Map<String, Object?> json) =>",
      "\t\t_\\$$1FromJson(json);",
      "}"
    ]
  },
  "freezed part": {
    "description": "snip freezed part",
    "prefix": [
      "snip_freezed_part"
    ],
    "body": [
      "part '$TM_FILENAME_BASE.freezed.dart';"
    ]
  },
  "generate part": {
    "description": "snip generate part",
    "prefix": [
      "snip_gen_part"
    ],
    "body": [
      "part '$TM_FILENAME_BASE.g.dart';"
    ]
  }
}
